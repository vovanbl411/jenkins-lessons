pipeline {

  agent { label 'mix-slave' }
  
  stages {
    stage('Run python') {
      steps {
        container('python') {
          sh '''
            python3 --version
            sleep 50
          '''
        }
      }
    }
    stage('Run java') {
      steps {
        container('java') {
            sh '''
                java --version
                sleep 50
            '''
        }
      }
    }
  }
}